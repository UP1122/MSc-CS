CREATE TABLE Inventory (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    ProductName VARCHAR(30) NOT NULL,
    Description TEXT NOT NULL,
    Price DECIMAL(10,2) NOT NULL,
    Quantity INT,
    create_date TEXT NOT NULL DEFAULT CURRENT_TIMESTAMP
);

